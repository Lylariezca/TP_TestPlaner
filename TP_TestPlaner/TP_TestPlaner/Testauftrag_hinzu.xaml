            <ComboBoxItem Content="Getestet"/>
        </ComboBox>
        <ComboBox x:Name="comboBox3" HorizontalAlignment="Left" Margin="250,327,0,0" VerticalAlignment="Top" Width="355"/>
        <TextBox x:Name="textBox3" HorizontalAlignment="Left" Height="23" Margin="250,360,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="355"/>
        <Label x:Name="label11" Content="Tester: " HorizontalAlignment="Left" Margin="10,389,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="comboBox4" HorizontalAlignment="Left" Margin="250,393,0,0" VerticalAlignment="Top" Width="355"/>
        <Label x:Name="label12" Content="Gemini-ID: " HorizontalAlignment="Left" Margin="10,45,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox4" HorizontalAlignment="Left" Height="23" Margin="250,48,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="355"/>

