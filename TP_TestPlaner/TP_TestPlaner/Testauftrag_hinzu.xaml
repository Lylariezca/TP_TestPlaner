        <Label x:Name="label10" Content="Testauftrag-Ersteller:" HorizontalAlignment="Left" Margin="10,358,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Content="Abbrechen" HorizontalAlignment="Left" Margin="10,435,0,0" VerticalAlignment="Top" Width="75" Click="button_Click"/>
        <Button x:Name="button1" Content="Speichern" HorizontalAlignment="Left" Margin="530,435,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="250,17,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="355"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Height="23" Margin="250,79,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="355"/>
        <TextBox x:Name="textBox2" HorizontalAlignment="Left" Height="23" Margin="250,110,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="355"/>
        <ComboBox x:Name="comboBox" HorizontalAlignment="Left" Margin="250,141,0,0" VerticalAlignment="Top" Width="355">
            <ComboBoxItem Content="Low"/>
            <ComboBoxItem Content="Medium"/>
            <ComboBoxItem Content="High"/>
        </ComboBox>
        <ComboBox x:Name="comboBox1" HorizontalAlignment="Left" Margin="250,172,0,0" VerticalAlignment="Top" Width="355">
            <ComboBoxItem Content="Smoke-Test"/>
            <ComboBoxItem Content="System-Test"/>
            <ComboBoxItem Content="Last-Test"/>
        </ComboBox>
        <DatePicker HorizontalAlignment="Left" Margin="250,203,0,0" VerticalAlignment="Top" Width="355"/>
        <DatePicker HorizontalAlignment="Left" Margin="250,234,0,0" VerticalAlignment="Top" Width="355"/>
        <DatePicker HorizontalAlignment="Left" Margin="250,265,0,0" VerticalAlignment="Top" Width="355"/>
        <ComboBox x:Name="comboBox2" HorizontalAlignment="Left" Margin="250,296,0,0" VerticalAlignment="Top" Width="355">
            <ComboBoxItem Content="Erstellt"/>
            <ComboBoxItem Content="Zugewiesen"/>
            <ComboBoxItem Content="In Durchführung"/>
