        mc:Ignorable="d"
        Title="Testauftrag hinzufügen" Height="507" Width="639">
    <Grid>
        <Label x:Name="label" Content="Titel: " HorizontalAlignment="Left" Margin="10,14,0,0" VerticalAlignment="Top"/>
